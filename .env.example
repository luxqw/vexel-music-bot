# Discord настройки для DEV
DISCORD_TOKEN=your-dev-bot-token-here
GUILD_ID=your-test-guild-id

# YouTube DL настройки
YDL_OPTS='{"format": "bestaudio"}'

# Dev специфичные настройки
DEBUG=true
LOG_LEVEL=DEBUG
ENVIRONMENT=development

# Музыкальные настройки для dev
MAX_PLAYLIST_SIZE=50
MAX_QUEUE_SIZE=100
DEFAULT_VOLUME=0.3

# Dev фичи
ENABLE_DEBUG_COMMANDS=true
ENABLE_METRICS=true
